{
  "rules": {
    ".read": true,
    ".write": true,
    "events": {
      "$event": {
        "name": {
          ".validate": "newData.isString() && newData.val().length > 0 && newData.val().length <= 255"
        },
        "earliest": {
          ".validate": "newData.isNumber() && newData.val() > 0"
        },
        "latest": {
          ".validate": "newData.isNumber() && newData.val() > 0"
        }
      }
    }
  }
}